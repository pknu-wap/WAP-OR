<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="82dp"
    android:background="@drawable/rounded_border5">
    <TextView
        android:id="@+id/account_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10.12"
        style="@style/account_date_style"
        android:layout_marginTop="31.5dp"
        android:layout_marginStart="16dp"
        android:background="@android:color/transparent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/account_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#식사"
        style="@style/account_category_style"
        android:layout_marginStart="23dp"
        android:background="@android:color/transparent"
        app:layout_constraintTop_toTopOf="@id/account_date"
        app:layout_constraintStart_toEndOf="@id/account_date"
        />
    <TextView
        android:id="@+id/account_deposit_cash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-5,800원"
        style="@style/account_deposit_cash_style"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="29dp"
        android:background="@android:color/transparent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <TextView
        android:id="@+id/account_remainder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0원"
        style="@style/account_remainder_style"
        android:layout_marginTop="7dp"
        android:background="@android:color/transparent"
        app:layout_constraintTop_toBottomOf="@+id/account_deposit_cash"
        app:layout_constraintEnd_toEndOf="@id/account_deposit_cash"
        />

</androidx.constraintlayout.widget.ConstraintLayout>