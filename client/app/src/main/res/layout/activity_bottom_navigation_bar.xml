<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:background="#FFFFFF"
    android:paddingTop="15dp"
    android:paddingBottom="15dp"
    >
    <!-- 첫 번째 버튼 -->
    <ImageButton
        android:id="@+id/mypage_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="#FFFFFF"
        android:layout_marginStart="31dp"
        android:src="@drawable/mypage_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <!-- 두 번째 버튼 -->
    <ImageButton
        android:id="@+id/accountpage_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="113dp"
        android:background="#FFFFFF"
        android:src="@drawable/account_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <!-- 세 번째 버튼 -->
    <ImageButton
        android:id="@+id/home_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="196dp"
        android:background="#FFFFFF"
        android:src="@drawable/home_icon_selected"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <!-- 네 번째 버튼 -->
    <ImageButton
        android:id="@+id/likepage_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="281dp"
        android:background="#FFFFFF"
        android:src="@drawable/like_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />
    <!-- 다섯 번째 버튼 -->
    <ImageButton
        android:id="@+id/analysispage_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="368dp"
        android:background="#FFFFFF"
        android:src="@drawable/analysis_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>